{
"manifest_version": 2,

"name":"2ManyTabs",
"description": "The basic boilerplate for creating a working chrome extension.",
"version":"0.0.0.0",

"browser_action":{
	"default_icon": "icons/icon.png",
	"default_popup":"popup.html"
},

"background": {
	"scripts": ["scripts/background.js"]
},

"content_scripts":[
	{
		"matches":["<all_urls>"],
		"js":["scripts/content.js"]
	}
],

"permissions":["tabs","storage"]
}
